{
	"name": "rifa-front",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"check": "tsc --noEmit",
		"start": "next start",
		"clean": "trash .next",
		"lint": "next lint",
		"build-storybook": "storybook build",
		"build": "cp .env.production .env.production.local && next build && next start",
		"build:dev": "cp .env.development .env.production.local && next build && next start",
		"build:staging": "cp .env.staging .env.production.local && next build && next start",
		"storybook": "storybook dev -p 6006",
		"test": "jest"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"eslint --fix",
			"git add"
		]
	},
	"dependencies": {
		"@headlessui/react": "^1.7.18",
		"@radix-ui/react-accordion": "^1.1.2",
		"@tanstack/react-query": "^5.24.1",
